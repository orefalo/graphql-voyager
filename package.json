{
  "name": "graphql-voyager",
  "type": "commonjs",
  "version": "1.0.0-rc.31",
  "description": "GraphQL introspection viewer",
  "main": "dist/voyager.lib.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "webpack serve --open --config webpack.dev.mjs",
    "build:dev": "NODE_ENV=development webpack --config webpack.dev.mjs --mode=development",
    "build:prod": "NODE_ENV=production webpack --config webpack.prod.mjs --mode=production",
    "clean": "run-s clean:*",
    "clean:release": "rimraf dist",
    "clean:demo": "rimraf demo-dist",
    "bundle": "run-s bundle:*",
    "bundle:lib": "webpack --env.lib --mode=production --config build/webpack.js",
    "bundle:standalone": "webpack --mode=production --config build/webpack.js -p",
    "bundledemo": "run-s bundledemo:*",
    "bundledemo:1": "npm run clean:demo",
    "bundledemo:2": "mkdir demo-dist",
    "bundledemo:3": "cp dist/voyager.css demo-dist",
    "bundledemo:4": "cp dist/voyager.min.js demo-dist",
    "bundledemo:5": "cp dist/voyager.worker.js demo-dist",
    "bundledemo:6": "cp src/index.html demo-dist",
    "bundledemo:7": "cd demo-dist && html-minifier-terser --minify-css 1 --minify-js 1 --use-short-doctype index.html > index1.html",
    "bundledemo:8": "cd demo-dist && inline-stylesheets index1.html .",
    "bundledemo:9": "cd demo-dist && wget https://cdn.jsdelivr.net/npm/react@16/umd/react.production.min.js",
    "bundledemo:10": "cd demo-dist && wget https://cdn.jsdelivr.net/npm/react-dom@16/umd/react-dom.production.min.js",
    "bundledemo:11": "cd demo-dist && inline-script-tags index.html .",
    "bundledemo:12": "cd demo-dist && rm index1.html react* voyager.min.js voyager.css",
    "blabla:7": "npm run clean:demo && NODE_ENV=production webpack -p --config build/webpack.demo.js",
    "build:release": "npm run bundle && npm run declarations",
    "prettier": "prettier --write --list-different .",
    "prettier:check": "prettier --check .",
    "declarations": "tsc --emitDeclarationOnly -p tsconfig.lib.json"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/plugin-transform-arrow-functions": "7.16.0",
    "@babel/plugin-transform-block-scoping": "7.16.0",
    "@babel/plugin-transform-classes": "7.16.0",
    "@babel/plugin-transform-destructuring": "7.16.0",
    "@types/classnames": "2.3.1",
    "@types/clipboard": "2.0.7",
    "@types/commonmark": "0.27.5",
    "@types/lodash": "4.14.177",
    "@types/prop-types": "15.7.4",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/webpack": "5.28.0",
    "babel-loader": "8.2.3",
    "css-loader": "6.5.1",
    "extract-loader": "^5.1.0",
    "graphql": "15.7.2",
    "html-minifier-terser": "^6.1.0",
    "html-webpack-plugin": "5.5.0",
    "inline-scripts": "^1.7.4",
    "mini-css-extract-plugin": "^2.4.5",
    "npm-run-all": "4.1.5",
    "postcss-cssnext": "3.1.1",
    "postcss-import": "14.0.2",
    "postcss-loader": "6.2.1",
    "postcss-variables-loader": "6.0.0",
    "prettier": "2.5.1",
    "raw-loader": "4.0.2",
    "react": "16.8.6",
    "react-clipboard.js": "2.0.16",
    "react-dom": "16.8.6",
    "react-svg-loader": "3.0.3",
    "rimraf": "3.0.2",
    "style-loader": "3.3.1",
    "svg-as-symbol-loader": "1.2.2",
    "ts-loader": "9.2.6",
    "typescript": "4.5.2",
    "webpack-dev-server": "4.6.0",
    "webpack-graphql-loader": "^1.0.2",
    "webpack-node-externals": "3.0.0",
    "worker-plugin": "^5.0.1",
    "webpack-merge": "^5.8.0",
    "clean-webpack-plugin": "^4.0.0",
    "prerender-loader": "^1.3.0",
    "file-loader": "^6.2.0",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "worker-loader": "^3.0.8"
  },
  "peerDependencies": {
    "graphql": ">=14.0.0",
    "react": ">=15.4.2",
    "react-dom": ">=15.4.2"
  },
  "dependencies": {
    "@f/animate": "^1.0.1",
    "@material-ui/core": "^4.12.3",
    "classnames": "^2.3.1",
    "clipboard": "^2.0.8",
    "commonmark": "^0.30.0",
    "lodash": "^4.17.21",
    "object-hash": "^2.2.0",
    "prop-types": "^15.7.2",
    "svg-pan-zoom": "^3.6.1",
    "@aduh95/viz.js": "^3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/APIs-guru/graphql-voyager.git"
  }
}
